<module rename-to="gvizapi">

  <inherits name='com.google.gwt.user.User'/>
  <inherits name='org.timepedia.chronoscope.Chronoscope'/>
  <inherits name="com.google.gwt.http.HTTP" />
  <inherits name="org.timepedia.chronoscope.gviz.api.GVizChronoscopeAPI" />
  <entry-point class="org.timepedia.chronoscope.gviz.api.client.GVizChronoscopeAPI" />
  <set-property name="export" value="yes"/>
  <set-property name="user.agent" value="gecko1_8,safari"/>
  <set-property name="hasflash" value="yes"/>
